{
  "name": "seqr",
  "version": "1.0.3",
  "description": "A small ES6 package for execute functions sequentially.",
  "repository": "joseluisq/seqr",
  "license": "MIT",
  "author": {
    "name": "JosÃ© Luis Quintana",
    "url": "http://git.io/joseluisq"
  },
  "keywords": [
    "sequential",
    "exec",
    "functions",
    "collection",
    "enqueue",
    "dequeue",
    "peek",
    "es6"
  ],
  "main": "dist/seqr.js",
  "scripts": {
    "build": "babel index.js | banr-stream > dist/seqr.js && cat dist/seqr.js | uglifyjs -mc | banr-stream > dist/seqr.min.js",
    "build-test": "npm run build && npm test",
    "test": "tape test.js"
  },
  "dependencies": {
    "quek": "^1.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "banr-stream": "^1.0.3",
    "tape": "^4.6.3",
    "uglify-js": "^2.7.4"
  },
  "babel": {
    "presets": ["es2015"],
    "plugins": [
      [
        "transform-es2015-modules-umd", {
        "globals": {
          "quek": "Quek"
        },
        "exactGlobals": true
      }]
    ],
    "moduleId": "Seqr"
  }
}
