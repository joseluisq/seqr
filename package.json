{
  "name": "seqr",
  "version": "1.0.5",
  "description": "A small ES6 package for execute functions sequentially.",
  "repository": "joseluisq/seqr",
  "license": "MIT",
  "author": {
    "name": "JosÃ© Luis Quintana",
    "url": "http://git.io/joseluisq"
  },
  "keywords": [
    "sequential",
    "exec",
    "functions",
    "collection",
    "enqueue",
    "dequeue",
    "peek",
    "es6"
  ],
  "main": "./seqr.js",
  "engines": {
    "node": ">= 4"
  },
  "scripts": {
    "start": "webpack-dev-server -d --content-base ./ --inline seqr.js",
    "build:lib": "babel ./src -d . --ignore 'test.js'",
    "build:umd": "webpack src/seqr.js dist/seqr.js",
    "build:min": "webpack -p src/seqr.js dist/seqr.min.js",
    "build": "node ./scripts/build.js",
    "prepublish": "node ./scripts/build.js",
    "release": "node ./scripts/release.js",
    "lint": "eslint src",
    "test": "npm run lint && jest --coverage",
    "coverage": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "dependencies": {
    "quek": "^1.1.2"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^20.0.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-1": "^6.16.0",
    "coveralls": "^2.11.15",
    "eslint": "^3.11.1",
    "gzip-size": "^3.0.0",
    "in-publish": "^2.0.0",
    "jest-cli": "^20.0.2",
    "pretty-bytes": "^4.0.2",
    "readline-sync": "^1.4.5",
    "webpack": "^2.3.0",
    "webpack-dev-server": "^2.4.3"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-1"
    ],
    "plugins": [
      "dev-expression"
    ]
  },
  "jest": {
    "verbose": true,
    "testPathIgnorePatterns": [
      "/(node_modules|scripts|dist)/"
    ],
    "testPathDirs": [
      "src"
    ],
    "testRegex": "test.(js|jsx|spec)$"
  }
}
